{
    "name": "seguce92/laravel-dompdf",
    "description": "A DOMPDF Wrapper for Laravel forks barryvdh/laravel-dompdf package",
    "license": "MIT",
    "type": "library",
    "keywords": ["laravel", "dompdf", "pdf"],
    "authors": [
        {
            "name": "Sergio Gualberto Cruz Espinoza",
            "email": "seguce92@gmail.com",
            "homepage": "https://sergiocruzes.com/"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "^5.5|^6",
        "dompdf/dompdf": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "Seguce92\\DomPDF\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seguce92\\DomPDF\\ServiceProvider"
            ],
            "aliases": [
                "Seguce92\\DomPDF\\Facade"
            ]
        }
    }
}
